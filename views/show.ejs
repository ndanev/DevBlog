<% include partials/header %>

<div class="container">
    <h1><%= blog.title %></h1>
    <img class="img-thumbnail img-fluid" src="<%= blog.image %>">
    <p><%= blog.body %></p>
    <span><%= blog.created.toDateString() %></span>
    <a class="ui purple basic button" href="/blogs/<%= blog._id %>">Read More</a>
</div>

<% include partials/footer %>